{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Small dashboard for exploring data",
  "main": "main.js",
  "scripts": {
    "start": "npm i && npm run devserver",
    "start-dev": "npm i && npm-run-all --parallel devserver sass-dev",
    "test": "jest",
    "devserver": "live-server",
    "sass-dev": "sass --watch --update --style=expanded styles.scss:styles.css",
    "sass-prod": "sass --no-source-map --style=compressed styles.scss:styles.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "babel-jest": "^29.2.2",
    "jest": "^29.2.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  }
}
