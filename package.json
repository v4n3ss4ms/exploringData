{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Small dashboard for exploring data",
  "main": "main.js",
  "scripts": {
    "start": "npm-run-all --parallel devserver sass-dev",
    "test": "npm i && mocha *.test.js",
    "devserver": "live-server",
    "sass-dev": "sass --watch --update --style=expanded styles.scss:styles.css",
    "sass-prod": "sass --no-source-map --style=compressed styles.scss:styles.css"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "live-server": "^1.2.1",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.55.0"
  }
}
